{
  "name": "eslint-config-lets-go",
  "version": "0.2.1",
  "description": "An ESLint config that aims to help *and not be annoying*",
  "main": "index.js",
  "eslintConfig": {
    "extends": [
      "eslint-config-lets-go"
    ],
    "env": {
      "node": true,
      "es2023": true
    },
    "parserOptions": {
      "ecmaVersion": 2023,
      "sourceType": "module"
    },
    "globals": {
      "Buffer": true,
      "console": true,
      "process": true,
      "setTimeout": true,
      "clearTimeout": true,
      "setInterval": true,
      "clearInterval": true,
      "setImmediate": true,
      "clearImmediate": true,
      "global": true
    }
  },
  "scripts": {
    "lint": "npx eslint example-issues/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Munawwar/eslint-config-lets-go.git"
  },
  "keywords": [
    "eslint",
    "configs"
  ],
  "author": "Munawwar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Munawwar/eslint-config-lets-go/issues"
  },
  "homepage": "https://github.com/Munawwar/eslint-config-lets-go#readme",
  "dependencies": {
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-lets-go": "file:.",
    "eslint-plugin-import-x": ">=3"
  },
  "peerDependencies": {
    "eslint": "*",
    "eslint-plugin-import-x": ">=3"
  }
}
